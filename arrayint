class ArrayInt
{
	public static void main (String []args)
	{
		int int2Arr [][] = new int [4] [5];
		int k=0;
		for (int i=0; i<4; i++)
		{
			for (int j=0; j<5; j++)
			{
				int2Arr[i][j] = k++;
				
			}
		}
		for (int i=0; i<4; i++)
		{
			for (int j=0; j<5; j++)
			{
				System.out.print(int2Arr [i][j] + " ");
			}
			System.out.println();
		}
	}
}
